{
  "name": "ReactWebpackNode",
  "version": "1.0.0",
  "description": "Boilerplate for react webpack node",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all --parallel open lint:watch",
    "open": "node index.js",
    "lint": "node_modules/.bin/esw webpack.config.* src",
    "lint:watch": "npm run lint -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "build:html": "node server/buildHtml.js",
    "prebuild": "npm-run-all clean-dist lint build:html",
    "build": "node server/build.js"
  },
  "author": "Abhirav Kumar",
  "license": "MIT",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-react-display-name": "^2.0.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-register": "^6.26.0",
    "bootstrap": "^4.1.3",
    "cheerio": "^1.0.0-rc.2",
    "compression": "^1.7.3",
    "css-loader": "^1.0.0",
    "eslint": "^5.5.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-react": "^7.11.1",
    "eslint-watch": "^4.0.2",
    "express": "^4.16.3",
    "file-loader": "^2.0.0",
    "jquery": "^3.3.1",
    "mini-css-extract-plugin": "^0.4.2",
    "node-sass": "^4.9.3",
    "npm-run-all": "^4.1.3",
    "popper.js": "^1.14.4",
    "react": "^16.5.0",
    "react-dom": "^16.5.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.0",
    "url-loader": "^1.1.1",
    "webpack": "^4.18.0",
    "webpack-dev-middleware": "^3.3.0",
    "webpack-hot-middleware": "^2.23.1"
  }
}
